{
  "name": "Basic SDXL",
  "description": "Simple SDXL text-to-image workflow with standard settings",
  "enableInSimple": true,
  "workflow": {},
  "prompt": {
    "1": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "${model}"
      }
    },
    "2": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${prompt}",
        "clip": ["1", 1]
      }
    },
    "3": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${negative_prompt}",
        "clip": ["1", 1]
      }
    },
    "4": {
      "class_type": "EmptyLatentImage",
      "inputs": {
        "width": "${width}",
        "height": "${height}",
        "batch_size": "${batch_size}"
      }
    },
    "5": {
      "class_type": "KSampler",
      "inputs": {
        "model": ["1", 0],
        "positive": ["2", 0],
        "negative": ["3", 0],
        "latent_image": ["4", 0],
        "seed": "${seed}",
        "steps": "${steps}",
        "cfg": "${cfg_scale}",
        "sampler_name": "${sampler}",
        "scheduler": "${scheduler}",
        "denoise": 1.0
      }
    },
    "6": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["5", 0],
        "vae": ["1", 2]
      }
    },
    "7": {
      "class_type": "SaveImage",
      "inputs": {
        "images": ["6", 0],
        "filename_prefix": "ERI"
      }
    }
  },
  "customParams": {
    "prompt": {
      "name": "Prompt",
      "type": "text",
      "description": "The main prompt describing what you want to generate",
      "default": "a beautiful landscape, highly detailed, masterpiece",
      "group": "Core"
    },
    "negative_prompt": {
      "name": "Negative Prompt",
      "type": "text",
      "description": "What to avoid in the generated image",
      "default": "ugly, blurry, low quality, worst quality",
      "group": "Core"
    },
    "seed": {
      "name": "Seed",
      "type": "integer",
      "description": "Random seed (-1 for random)",
      "default": -1,
      "min": -1,
      "max": 2147483647,
      "group": "Core"
    },
    "steps": {
      "name": "Steps",
      "type": "integer",
      "description": "Number of sampling steps",
      "default": 25,
      "min": 1,
      "max": 150,
      "group": "Sampling"
    },
    "cfg_scale": {
      "name": "CFG Scale",
      "type": "decimal",
      "description": "Classifier-free guidance scale",
      "default": 7.0,
      "min": 1.0,
      "max": 30.0,
      "step": 0.5,
      "group": "Sampling"
    },
    "width": {
      "name": "Width",
      "type": "integer",
      "description": "Image width in pixels",
      "default": 1024,
      "min": 64,
      "max": 4096,
      "step": 64,
      "group": "Dimensions"
    },
    "height": {
      "name": "Height",
      "type": "integer",
      "description": "Image height in pixels",
      "default": 1024,
      "min": 64,
      "max": 4096,
      "step": 64,
      "group": "Dimensions"
    },
    "sampler": {
      "name": "Sampler",
      "type": "dropdown",
      "description": "Sampling algorithm",
      "default": "euler",
      "values": ["euler", "euler_ancestral", "dpmpp_2m", "dpmpp_2m_sde", "dpmpp_sde", "ddim", "uni_pc"],
      "group": "Sampling",
      "advanced": true
    },
    "scheduler": {
      "name": "Scheduler",
      "type": "dropdown",
      "description": "Noise scheduler",
      "default": "normal",
      "values": ["normal", "karras", "exponential", "sgm_uniform", "simple", "ddim_uniform"],
      "group": "Sampling",
      "advanced": true
    }
  },
  "paramValues": {
    "prompt": "a beautiful landscape, highly detailed, masterpiece",
    "negative_prompt": "ugly, blurry, low quality, worst quality",
    "seed": -1,
    "steps": 25,
    "cfg_scale": 7.0,
    "width": 1024,
    "height": 1024,
    "sampler": "euler",
    "scheduler": "normal",
    "batch_size": 1,
    "model": ""
  }
}
