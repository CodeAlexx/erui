{
  "name": "Img2Img",
  "description": "Image-to-image workflow with denoise control",
  "tags": ["img2img", "checkpoint"],
  "enable_in_generate": true,
  "created": "2026-01-07T00:00:00Z",
  "modified": "2026-01-07T00:00:00Z",

  "parameters": {
    "prompt": {"type": "text", "label": "Prompt", "default": "enhanced, detailed"},
    "negative_prompt": {"type": "text", "label": "Negative Prompt", "default": ""},
    "model": {"type": "model", "label": "Model", "default": ""},
    "init_image": {"type": "image", "label": "Input Image", "default": ""},
    "denoise": {"type": "decimal", "label": "Denoise", "default": 0.75, "min": 0, "max": 1},
    "seed": {"type": "integer", "label": "Seed", "default": -1},
    "steps": {"type": "integer", "label": "Steps", "default": 20},
    "cfg": {"type": "decimal", "label": "CFG Scale", "default": 7.0},
    "sampler": {"type": "select", "label": "Sampler", "default": "euler"},
    "scheduler": {"type": "select", "label": "Scheduler", "default": "normal"}
  },

  "prompt": {
    "4": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "${model}"
      }
    },
    "15": {
      "class_type": "LoadImage",
      "inputs": {
        "image": "${init_image}"
      }
    },
    "16": {
      "class_type": "VAEEncode",
      "inputs": {
        "pixels": ["15", 0],
        "vae": ["4", 2]
      }
    },
    "6": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${prompt}",
        "clip": ["4", 1]
      }
    },
    "7": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${negative_prompt}",
        "clip": ["4", 1]
      }
    },
    "3": {
      "class_type": "KSampler",
      "inputs": {
        "seed": "${seed:-1}",
        "steps": "${steps:20}",
        "cfg": "${cfg:7.0}",
        "sampler_name": "${sampler:euler}",
        "scheduler": "${scheduler:normal}",
        "denoise": "${denoise:0.75}",
        "model": ["4", 0],
        "positive": ["6", 0],
        "negative": ["7", 0],
        "latent_image": ["16", 0]
      }
    },
    "8": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["3", 0],
        "vae": ["4", 2]
      }
    },
    "9": {
      "class_type": "SaveImage",
      "inputs": {
        "filename_prefix": "EriUI_Img2Img",
        "images": ["8", 0]
      }
    }
  }
}
