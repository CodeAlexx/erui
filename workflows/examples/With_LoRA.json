{
  "name": "With LoRA",
  "description": "Text-to-image with LoRA support",
  "tags": ["text2img", "lora", "checkpoint"],
  "enable_in_generate": true,
  "created": "2026-01-07T00:00:00Z",
  "modified": "2026-01-07T00:00:00Z",

  "parameters": {
    "prompt": {"type": "text", "label": "Prompt", "default": "a beautiful portrait"},
    "negative_prompt": {"type": "text", "label": "Negative Prompt", "default": ""},
    "model": {"type": "model", "label": "Model", "default": ""},
    "lora_name": {"type": "lora", "label": "LoRA", "default": ""},
    "lora_strength": {"type": "decimal", "label": "LoRA Strength", "default": 1.0, "min": -2, "max": 2},
    "seed": {"type": "integer", "label": "Seed", "default": -1},
    "steps": {"type": "integer", "label": "Steps", "default": 20},
    "cfg": {"type": "decimal", "label": "CFG Scale", "default": 7.0},
    "width": {"type": "integer", "label": "Width", "default": 1024},
    "height": {"type": "integer", "label": "Height", "default": 1024},
    "sampler": {"type": "select", "label": "Sampler", "default": "euler"},
    "scheduler": {"type": "select", "label": "Scheduler", "default": "normal"}
  },

  "prompt": {
    "4": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "${model}"
      }
    },
    "20": {
      "class_type": "LoraLoader",
      "inputs": {
        "lora_name": "${lora_name}",
        "strength_model": "${lora_strength:1.0}",
        "strength_clip": "${lora_strength:1.0}",
        "model": ["4", 0],
        "clip": ["4", 1]
      }
    },
    "5": {
      "class_type": "EmptyLatentImage",
      "inputs": {
        "width": "${width:1024}",
        "height": "${height:1024}",
        "batch_size": 1
      }
    },
    "6": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${prompt}",
        "clip": ["20", 1]
      }
    },
    "7": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "${negative_prompt}",
        "clip": ["20", 1]
      }
    },
    "3": {
      "class_type": "KSampler",
      "inputs": {
        "seed": "${seed:-1}",
        "steps": "${steps:20}",
        "cfg": "${cfg:7.0}",
        "sampler_name": "${sampler:euler}",
        "scheduler": "${scheduler:normal}",
        "denoise": 1.0,
        "model": ["20", 0],
        "positive": ["6", 0],
        "negative": ["7", 0],
        "latent_image": ["5", 0]
      }
    },
    "8": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["3", 0],
        "vae": ["4", 2]
      }
    },
    "9": {
      "class_type": "SaveImage",
      "inputs": {
        "filename_prefix": "EriUI_LoRA",
        "images": ["8", 0]
      }
    }
  }
}
